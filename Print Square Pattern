//print square pattern

//using for
public class javabasics{
    public static void main(String args[]){
     
       for(int line=1 ;line<=4;line++){
        System.out.println("****");
       }
       }
    } 

//using while
public class javabasics{
    public static void main(String args[]){
       int line=1;
       while (line<=4)
       {System.out.println("****");
        line++;
       }
    }
}
    
