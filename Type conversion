//Type conversion..widening or implicit conversion

/*a)type  compatible
 *b)destination type >source type
 */


import java.util.*;

public class javabasics {
      public static void main(String args[])
      {
        // int a=5;
         //long b=a;
         long a=5;
         int b=a;
         System.out.println(b);
      }

}
