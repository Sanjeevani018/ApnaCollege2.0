import java.util.*;

public class javaBasics{
    public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the which table you want : ");
     int n =sc.nextInt();
     int mul;
     for (int i =1 ; i<=10;i++){
        mul=n*i;
        System.out.println(n + " * " + i  + " = " + mul);
     }
    }
}
           
